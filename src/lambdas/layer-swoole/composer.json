{
    "name": "deminy/test-layer-swoole",
    "homepage": "https://github.com/deminy/aws-cdk-workshop",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "bref/bref": "~2.1"
    },
    "require-dev": {
        "swoole/ide-helper": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Bref\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.0"
        },
        "apcu-autoloader": true,
        "discard-changes": true,
        "optimize-autoloader": true,
        "sort-packages": true,
        "process-timeout": 0
    },
    "prefer-stable": true
}
